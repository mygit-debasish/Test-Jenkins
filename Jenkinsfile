pipeline {
	agent any 

	 stages {
	 	
		 stage("build"){
			 
	 		steps {
	 			echo 'Building the applications ...'
	 		}
	 	}
	 	stage("Test") {
	 		steps {
	 			echo 'Testing the applications..'
	 		}
	 	}

	 	Stage("Deploy") {
	 		steps {
	 			echo 'Deploying applications..'
	 		}
	 	}

	 }

		
	}
